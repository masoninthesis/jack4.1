        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "gravityforms/gravityformstwilio",
        "version": "2.1.3",
        "type": "wordpress-plugin",
        "dist": {
          "type": "zip",
          "url": "http://s3.amazonaws.com/gravityforms/addons/twilio/gravityformstwilio_2.1.3.zip?AWSAccessKeyId=1603BBK66770VCSCJSG2&Expires=1455673861&Signature=LAD5Tlr5S%2FhEpJIhkGx%2B5K8gBeM%3D"
