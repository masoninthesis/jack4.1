        "type": "wordpress-plugin",
        "dist": {
          "type": "zip",
          "url": "http://s3.amazonaws.com/gravityforms/addons/stripe/gravityformsstripe_2.0.2.zip?AWSAccessKeyId=1603BBK66770VCSCJSG2&Expires=1455673861&Signature=poE27gEAFK4i0Kqg0O9idMk92Ts%3D"
        }
      }
    }
  ],
  "require": {
    "php": ">=5.5",
    "composer/installers": "~1.0.12",
    "vlucas/phpdotenv": "^2.0.1",
    "johnpbloch/wordpress": "4.3.1",
    "roots/soil": "^3.6",
    "gravityforms/gravityforms": "1.9.16.5",
    "gravityforms/gravityformsmailchimp": "3.7.1",
    "gravityforms/gravityformszapier": "1.8",
    "gravityforms/gravityformstwilio": "2.1.3",
